{
  "name": "try-react-admin",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x",
    "npm": "7.x"
  },
  "dependencies": {
    "@emotion/react": "11.4.1",
    "normalize.css": "8.0.1",
    "ra-data-json-server": "3.18.3",
    "react-admin": "3.18.3",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "26.0.24",
    "@types/node": "16.10.3",
    "@types/query-string": "5.1.0",
    "@types/react": "17.0.27",
    "@types/react-dom": "17.0.9",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-jest": "24.5.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-sort-destructure-keys": "1.3.5",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "1.8.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "react": "17.0.2",
    "typescript": "4.4.3",
    "web-vitals": "2.1.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "eslint-print-config": "eslint --print-config src/index.tsx > misc/eslint-print-config.json",
    "lint-dry": "npm-run-all --parallel --continue-on-error lint:eslint-dry lint:tsc lint:format-dry",
    "lint:eslint-dry": "eslint -c .eslintrc.js --max-warnings=0 .",
    "lint:eslint": "eslint -c .eslintrc.js --fix --max-warnings=0 .",
    "lint:format-dry": "prettier --check .",
    "lint:format": "prettier --write .",
    "lint:tsc": "tsc --noEmit",
    "lint": "npm-run-all --parallel --continue-on-error lint:eslint lint:tsc lint:format",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "update-all": "npx npm-check-updates -u --reject \"/^(@types/jest|@types/node|@types/query-string)$/\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
